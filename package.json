    "eslint-plugin-unicorn": "^33.0.1",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "markdownlint-cli": "^0.27.1",
    "prettier": "^2.3.2",
    "semantic-release": "^17.4.4",
    "ts-jest": "^27.0.3",
