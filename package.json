{
  "name": "typedoc-plugin-param-names",
  "version": "2.0.0",
  "description": "Change the name of __namedParameters",
  "keywords": [
    "typedoc",
    "typedocplugin"
  ],
  "author": "cherryblossom (https://github.com/cherryblossom000)",
  "license": "MIT",
  "homepage": "https://github.com/cherryblossom/typedoc-plugin-param-names#readme",
  "repository": "github:cherryblossom000/typedoc-plugin-param-names",
  "bugs": "https://github.com/cherryblossom000/typedoc-plugin-param-names/issues",
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts"
  ],
  "main": "./dist/index",
  "types": "./dist/index.d.ts",
  "directories": {
    "lib": "./dist"
  },
  "scripts": {
    "build": "tsc -b",
    "build:dev": "tsc -b tsconfig.dev.json",
    "watch": "pnpm run build:dev -- -w",
    "lint": "eslint --max-warnings 0 --ext .ts .",
    "lint:dev": "pnpm run lint -- --cache",
    "lint:md": "markdownlint ."
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "peerDependencies": {
    "typedoc": ">=0.15.5 <0.21"
  },
  "devDependencies": {
    "@cherryblossom/eslint-config": "^13.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/types": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "commitizen": "^4.2.2",
    "commitlint": "^11.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.17.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.13",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unicorn": "^25.0.1",
    "husky": "^4.3.7",
    "markdownlint-cli": "^0.26.0",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.1",
    "typedoc": "^0.20.14",
    "typescript": "^4.1.3"
  }
}
